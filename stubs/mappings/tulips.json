{
  "request": {
    "method": "GET",
    "urlPathPattern": "/mdm/api?([a-z]*)",
    "queryParameters": {
      "filter": {
        "contains": "tulips"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "tulips.json"
  }
}
